<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00ACC1"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/titulo"
        android:layout_width="333dp"
        android:layout_height="32dp"
        android:background="#AEDDF3"
        android:breakStrategy="high_quality"
        android:foregroundTint="#F3F2F1"
        android:text="CATALOGO DE MOTOS"
        android:textAlignment="center"
        android:textColor="#FCEF0707"
        android:textSize="24sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.423"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022" />

    <TextView
        android:id="@+id/moto1"
        android:layout_width="169dp"
        android:layout_height="44dp"
        android:background="#A3EAF3"
        android:text="MT-09 MODELO 2023 PRECIO:57.500.000"
        android:textAlignment="viewStart"
        android:textColor="#050101"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.102"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.433" />

    <TextView
        android:id="@+id/moto2"
        android:layout_width="143dp"
        android:layout_height="46dp"
        android:background="#A3EAF3"
        android:text="MT10-SP MODELO 2023 PRECIO:94.500.000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.843"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.432" />

    <TextView
        android:id="@+id/moto3"
        android:layout_width="164dp"
        android:layout_height="53dp"
        android:background="#A3EAF3"
        android:text="Panigale-V2 MODELO 2024 PRECIO: 105.500.000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.117"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.867" />

    <TextView
        android:id="@+id/moto4"
        android:layout_width="145dp"
        android:layout_height="56dp"
        android:background="#A3EAF3"
        android:text="XDiavel Nera MODELO 2023 PRECIO: 84.950.000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.86"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.866" />

    <Button
        android:id="@+id/comprar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:text="comprar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.467"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.963" />

    <ImageButton
        android:id="@+id/m1"
        android:layout_width="164dp"
        android:layout_height="211dp"
        android:background="@android:color/transparent"
        android:contentDescription="97000"
        android:scaleType="fitCenter"
        android:src="@drawable/m1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.101"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.119"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/m2"
        android:layout_width="148dp"
        android:layout_height="210dp"
        android:background="@android:color/transparent"
        android:contentDescription="97000"
        android:scaleType="fitCenter"
        android:src="@drawable/m2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.857"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.117"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/m3"
        android:layout_width="154dp"
        android:layout_height="204dp"
        android:background="@android:color/transparent"
        android:contentDescription="97000"
        android:scaleType="fitCenter"
        android:src="@drawable/m3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.112"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.685"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/m4"
        android:layout_width="152dp"
        android:layout_height="201dp"
        android:background="@android:color/transparent"
        android:contentDescription="97000"
        android:scaleType="fitCenter"
        android:src="@drawable/m4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.872"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.681"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>